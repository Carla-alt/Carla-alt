public class Main{
    public void fullTrottle(){
        System.out.println("Apasa acceleratia!");
    }

    public void speed(int maxSpeed){
        System.out.println("Viteza maxima este:" + maxSpeed);
    }

    public static void main(String[] args){
        Main myCar=new Main();
        myCar.fullTrottle();
        myCar.speed(200);
    }
}
